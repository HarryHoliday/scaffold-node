{
  "name": "scaffold-node",
  "version": "1.0.0",
  "description": "simple scaffold for node",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --config webpack.config.js --watch",
    "build": "webpack --mode production --config webpack.config.js",
    "test": "ava",
    "watch:test": "ava --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "scaffold",
    "node"
  ],
  "author": "harry holiday <12356741@daum.net> (https://github.com/HarryHoliday)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/node": "^13.1.6",
    "@types/pino": "^5.15.1",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "ava": "^2.4.0",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-ava": "^9.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "nodemon-webpack-plugin": "^4.2.2",
    "prettier": "^1.19.1",
    "ts-config-webpack-plugin": "^1.4.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "pino": "^5.15.0",
    "pino-pretty": "^3.5.0"
  }
}
